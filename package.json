{
  "name": "ember-simple-auth-oidc",
  "version": "0.4.3",
  "description": "A Ember Simple Auth addon which implements the OpenID Connect Authorization Code Flow.",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/adfinis-sygroup/ember-simple-auth-oidc",
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "ember-ajax": "^5.0.0",
    "ember-auto-import": "^1.5.3",
    "ember-cli-babel": "^7.12.0",
    "ember-cli-htmlbars": "^4.0.8",
    "ember-get-config": "^0.2.4",
    "ember-simple-auth": "^2.1.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@adfinis-sygroup/semantic-release-config": "2.1.4",
    "@babel/core": "7.7.5",
    "@ember/jquery": "1.1.0",
    "@ember/optional-features": "1.1.0",
    "babel-eslint": "10.0.3",
    "broccoli-asset-rev": "3.0.0",
    "ember-cli": "3.13.1",
    "ember-cli-dependency-checker": "3.2.0",
    "ember-cli-inject-live-reload": "2.0.2",
    "ember-cli-mirage": "1.1.4",
    "ember-cli-sri": "2.1.1",
    "ember-cli-template-lint": "1.0.0-beta.3",
    "ember-cli-uglify": "3.0.0",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-export-application-global": "2.0.1",
    "ember-load-initializers": "2.1.1",
    "ember-maybe-import-regenerator": "0.1.6",
    "ember-qunit": "4.6.0",
    "ember-resolver": "6.0.0",
    "ember-source": "3.13.3",
    "ember-source-channel-url": "2.0.1",
    "ember-try": "1.3.0",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-ember": "7.7.1",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-prettier": "3.1.1",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "loader.js": "4.7.0",
    "prettier": "1.19.1",
    "qunit-dom": "0.9.2"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release": {
    "extends": "@adfinis-sygroup/semantic-release-config"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
